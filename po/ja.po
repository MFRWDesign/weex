#: src/weex.c:155
msgid ""
"usage: weex [OPTIONS] HOST...\n"
"\n"
"options:\n"
msgstr ""
"使用法: weex [オプション] ホスト...\n"
"\n"
"オプション:\n"

#: src/weex.c:156
msgid "  -c | --config-file=FILE   use an alternate configuration file\n"
msgstr "  -c | --config-file=FILE   別の設定ファイルを使用\n"

#: src/weex.c:157
msgid "  -w | --not-mkdir-weex     not make ~/.weex directory on startup\n"
msgstr ""
"  -w | --not-mkdir-weex     起動時に ~/.weex ディレクトリを作成しない\n"

#: src/weex.c:158
msgid "  -v | --view               display the host difinitions\n"
msgstr "  -v | --view               ホストの設定を表示\n"

#: src/weex.c:159
msgid "  -l | --list               display a listing of files needing update\n"
msgstr "  -l | --list               更新が必要なファイルの一覧を表示\n"

#: src/weex.c:160
msgid ""
"  -n | --nlist              display the number of files needing update\n"
msgstr "  -n | --nlist              更新が必要なファイルの数を表示\n"

#: src/weex.c:161
msgid "  -r | --rebuild-cache      rebuild a cache file\n"
msgstr "  -r | --rebuild-cache      キャッシュファイルを再構築\n"

#: src/weex.c:162
#, fuzzy
msgid ""
"  -R | --catch-up           rebuild a cache file as all files are updated\n"
msgstr ""
"  -f | --force              "
"更新されているかどうかにかかわらず全てのファイルを転送\n"

#: src/weex.c:163
msgid "  -u | --upload             transfer files from local to remote\n"
msgstr "  -u | --upload             ローカルからリモートへファイルを転送\n"

#: src/weex.c:164
msgid "  -d | --download           transfer files from remote to local\n"
msgstr "  -d | --download           リモートからローカルへファイルを転送\n"

#: src/weex.c:165
msgid "  -a | --all-hosts          process all defined hosts\n"
msgstr "  -a | --all-hosts          定義されている全てのホストを処理\n"

#: src/weex.c:166
#, fuzzy
msgid "  -C | --confirmation       request confirmation when change files\n"
msgstr ""
"  -C | --confirmation       ファイル/ディレクトリが変更される時に確認を要求\n"

#: src/weex.c:167
msgid "  -f | --force              transfer all files whether updated or not\n"
msgstr ""
"  -f | --force              "
"更新されているかどうかにかかわらず全てのファイルを転送\n"

#: src/weex.c:168
msgid ""
"  -t | --test               perform ordering simulation (not actually "
"change)\n"
msgstr "  -t | --test               処理をシミュレート (実際には変更しない)\n"

#: src/weex.c:169
msgid "  -s | --silent             suppress non-error messages\n"
msgstr "  -s | --silent             エラーメッセージ以外の表示を抑制\n"

#: src/weex.c:170
msgid "  -q | --quiet              suppress non-error messages\n"
msgstr "  -q | --quiet              エラーメッセージ以外の表示を抑制\n"

#: src/weex.c:171
msgid "  -p | --suppress-progress  suppress progress information\n"
msgstr "  -p | --suppress-progress  進捗情報表示を抑制\n"

#: src/weex.c:172
msgid "  -m | --monochrome         output messages in monochromee\n"
msgstr "  -m | --monochrome         メッセージをモノクロで表示\n"

#: src/weex.c:173
msgid "  -g | --debug              enable debug output\n"
msgstr "  -g | --debug              デバッグ出力を有効にする\n"

#: src/weex.c:174
msgid "  -h | --help               display this help and exit\n"
msgstr "  -h | --help               このヘルプを表示して終了\n"

#: src/weex.c:175
msgid "  -V | --version            display version information and exit\n"
msgstr "  -V | --version            バージョン情報を表示して終了\n"

